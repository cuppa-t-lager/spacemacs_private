# -*- mode: snippet -*-
# name: <cfcontent />
# key: cfco
# group: coldfusion
# --
<cfcontent
	type		= "${4:$$(yas-choose-value '("text/html" "text/plain" "text/json" "application/x-shockwave-flash" "application/msword" "image/jpeg"))text/html}"
	reset		= "${3:$$(yas-choose-value '("yes" "no"))no}"
	variable	= "${5:variable name}"
	deleteFile	= "${1:$$(yas-choose-value '("yes" "no"))no}"
	file		= "${2:filename}"
/>
